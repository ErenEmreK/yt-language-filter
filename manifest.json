{
    "manifest_version": 3,
    "name": "YouTube Language Filter",
    "version": "1.0",
    "description": "A Chrome extension to filter YouTube videos by language.",
    "permissions": [
      "activeTab",
      "scripting"
    ],
   
    
    "content_scripts": [
      {
        "matches": ["*://www.youtube.com/results*"],
        "js": ["content.js"]
      }
    ]
}
  