{
    "manifest_version": 3,
    "name": "Youtube Language Filter",
    "version": "1.0",
    "description": "Yt language filter",
    "permissions": [
        "activeTab",
        "storage",
        "declarativeNetRequest",
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": ["*://www.youtube.com/results*"],
            "js": ["content.js"]
        }
    ]
}